"VertexLitGeneric"
{
	// base material for laser cubes (shell, rings and core studs)

	$basetexture	BEE2/models/cube_color/hmw_laser_cube/laser_cube_tint
	$blendtintbybasealpha 1

	$bumpmap "paint/prop_paint_normal"
	$envmap "paint/paint_envmap_hdr"
	$envmaptint "[0.05 0.05 0.05]"
	$normalmapalphaenvmapmask 1
	$detail "paint/prop_paint_orange"
	$detailScale 1
	$detailblendmode 2
	$phong 1
	$phongexponent 50
	$phongfresnelranges "[1 2 4]"
	$phongboost 2
	$selfillum 1
	$nodecal 1
}
